{{#with project}}
<!DOCTYPE html>
<html lang="en">
	<head>

		<!-- Meta tags -->
		<meta charset="utf-8">
    	<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
		<meta name="description" content="Jared Suasin | {{name}}" />
		<meta name="keywords" content="Jared Suasin, visual design, interaction design, ui design, ux design, product design, front-end development, ux engineering">
		<meta property="og:title" content="Jared Suasin | {{name}}">
		<meta property="og:type" content="website"/>
		<meta property="og:image" content="https://www.jaredsuasin.com/images/portfolio-cover.png"/>
		<meta property="og:url" content="https://www.jaredsuasin.com/"/>
		<meta property="og:description" content="Jared Suasin - {{name}}">	
		
		<title>Jared Suasin &mdash; {{name}}</title>
		<link href="css/main.css" rel="stylesheet" type="text/css">
		<link href="https://fonts.googleapis.com/css?family=Montserrat:100,200,300,400,400i,500,600" rel="stylesheet">
		<link rel="stylesheet" href="fonts/stylesheet.css" type="text/css" charset="utf-8" />
	
		<!-- Favicons -->
        <link rel="apple-touch-icon" sizes="180x180" href="/favicons/apple-touch-icon.png">
        <link rel="icon" type="image/png" sizes="32x32" href="/favicons//favicon-32x32.png">
        <link rel="icon" type="image/png" sizes="16x16" href="/favicons//favicon-16x16.png">
        <link rel="manifest" href="/favicons//site.webmanifest">
        <link rel="mask-icon" href="/favicons//safari-pinned-tab.svg" color="#45af6e">
        <meta name="msapplication-TileColor" content="#da532c">
        <meta name="theme-color" content="#ffffff">

		<script src="./js/jquery-1.8.2.min.js"></script>
	
	</head>
	<body>
		<!--<div class="hero"></div>-->
		<canvas id="c"></canvas>
		<div class="page project">

			<!-- Header -->
			<div class="header">

				<!-- Name -->
				<div class="nav_title">
					<div class="nav_name">
						<a href="/"><h2>JARED SUASIN</h2></a>
					</div>
					<!--<p>product designer</p>-->
				</div>

				<!-- Navbar -->
				<div class="nav_group transparent">
					<a class="hover_sweep" href="/work">
						<div class="nav_item">WORK</div>
					</a>
					<a class="hover_sweep" href="/about">
						<div class="nav_item">ABOUT</div>
					</a>
					<a class="hover_sweep" target="_blank" href="/resume">
						<div class="nav_item">RESUME</div>
					</a>
				</div>
			</div> <!-- End header -->


			<!-- Project header -->
			<div class="section section_vertical transparent">
				<div class="hero_card"></div>
				<div class="project_intro">

					<!-- Project name & description -->
					<div class="project_intro_header">
						<h1>{{name}}</h1>
						<div class="tag_group">
							{{#each tags}}
								<div class="tag tag_default caption2">{{tag_name}}</div>
							{{/each}}
						</div>
					</div>
					<hr class="divider_thin">
				

					<div class="block_large description">
						<h3>{{{description}}}</h3>
					</div>

					<div class="block_medium blocks_horizontal distribute4">
						{{#each metadata}}
							<div class="labeled_data">
								<h6 class="caption2">{{data_name}}</h6>
								<div class="labeled_data_content">
									<p>{{{data_content}}}</p>
								</div>
							</div>
						{{/each}}
					</div>

				</div>
			</div>



			{{#each sections}}

				<!-- WIDE SECTION -->
				{{#if section_wide}}
					<!-- Section title -->
					{{#if section_header}}
						<div class="section section_horizontal section_header">
							<div class="section_title">
								<h1>{{title}}</h1>
							</div>
							<h3 class="section_description description">{{content}}</h3>
						</div>
					{{/if}}

					<!-- Section with body text -->
					{{#if section_content}}

						<div class="section section_horizontal section_header">
							<div class="section_title">
								<h1>{{title}}</h1>
							</div>
							<p class="section_description description">{{content}}</p>
						</div>
					{{/if}}

					<!-- Image section -->
					{{#if image}}
						<div class="section transparent image" style="background-image:url({{image_url}});"></div>
					{{/if}}


					<!-- Blocks -->
					{{#if if_blocks}}

						<!-- Flow by row -->
						{{#if flow_row}}
							<div class="blocks_horizontal {{mobile_before_after}}">
								{{#each blocks}}
									<div class="block_column {{card}} {{flex_span}} {{span_width}} {{position}}">
								
										<!-- Image -->
										{{#if image}}
											<!--<div class="block transparent image" style="background-image:url({{image_url}});"></div>-->
											{{#if caption_top}}
												<div class="caption {{caption_position}}">
													<h6>{{label}}</h6>
													<p>{{content}}</p>
												</div>
											{{/if}}
											<img class="image_fit {{gif}}" src="{{image_url}}">

											{{#if caption_bottom}}
												<div class="caption {{caption_position}}">
													<h6>{{label}}</h6>
													<p>{{content}}</p>
												</div>
											{{/if}}
										{{/if}}									

										<!-- Text -->
										{{#if text}}
											<h3>{{title}}</h3>
											<p class="description">{{{content}}}</p>
										{{/if}}

										<!-- Data block with description -->
										{{#if if_data_block_caption}}
												<div class="data_block">
													<h3 class="data_block_value">{{data}}</h3>
													<p class="caption1">{{label}}</p>
													<p class="data_block_description">{{description}}</p>
												</div>
												
										{{/if}}

										<!-- Mobile image caption -->
										{{#if caption}}
											<div class="mobile_caption labeled_data_large">
												<h5>{{label}}</h5>
												<p>{{{content}}}</p>
											</div>											
										{{/if}}

										<!-- Data block group -->
										{{#if if_data_block}}
											<h3>{{title}}</h3>
											<div class="data_block_group block_horizontal data_block_distribute2">
												{{#each data_blocks}}
													<div class="data_block">
														<h2 class="data_block_value">{{data}}</h2>
														<p class="caption1">{{label}}</p>
													</div>
												{{/each}}
											</div>
										{{/if}}

										<!-- Centered section -->
										{{#if if_centered_section}}
											<div class="span_row_center">
												<div class="span2"></div>
												<div class="span4">
													<h3>{{title}}</h3>
													<p class="description">{{{content}}}</p>
												</div>
												<div class="span2"></div>
										
											</div>
										{{/if}}	

										<!-- Right aligned section -->
										{{#if if_right_section}}
											<div class="span_row_right">
												<div class="span3 span_half"></div>
												<div class="transparent block_card span_half">
													<h3>{{title}}</h3>
													<p class="description">{{{content}}}</p>
												</div>
											</div>
										{{/if}}		

										<!-- Stacked blocks -->
										{{#if flow_column}}
											<div class="block_column blocks_vertical">
												{{#each blocks}}
													<div class="block_column {{card}}">
														
													<!-- Image -->
													{{#if image}}
														<!--<div class="block transparent image" style="background-image:url({{image_url}});"></div>-->
														{{#if caption_top}}
															<div class="caption {{caption_position}}">
																<h6>{{label}}</h6>
																<p>{{content}}</p>
															</div>
														{{/if}}
														<img class="image_fit" src="{{image_url}}">

														{{#if caption_bottom}}
															<div class="caption {{caption_position}}">
																<h6>{{label}}</h6>
																<p>{{content}}</p>
															</div>
														{{/if}}
													{{/if}}								

														<!-- Text -->
														{{#if text}}
															<h3>{{title}}</h3>
															<p class="description">{{{content}}}</p>
														{{/if}}

														<!-- Data block group -->
														{{#if if_data_block}}
															<h3>{{title}}</h3>
															<div class="data_block_group blocks_horizontal data_block_distribute2">
																{{#each data_blocks}}
																	<div class="data_block">
																		<h2 class="data_block_value">{{data}}</h2>
																		<p class="caption1">{{label}}</p>
																	</div>
																{{/each}}
															</div>
														{{/if}}		
													</div>
												{{/each}}
											</div>
											
										{{/if}}				
									</div>
								{{/each}}
							</div>
						{{/if}}
					{{/if}} <!-- End blocks -->

					{{#if if_divider}}
						<div class="divider_thin divider_section"></div>
					{{/if}}
				{{/if}} <!-- End section_wide -->
			{{/each}}

			<!-- Projects -->
			<div class="project_group">
				{{#each projects}}
					<div class="project_card transparent">
						<a href="{{url}}">
							<div class="project_card_image" style="background-image: url({{project_card_image}});"></div>
							<div class="project_card_content">
								<div class="tag_group">
									{{#each tags}}
										<div class="tag tag_default caption2">{{tag_name}}</div>
									{{/each}}
								</div>
								<h3>{{name}}</h3>
								<p>{{card_description}}</p>
								<!--<h6 class="project_card_CTA">VIEW CASE STUDY</h6>-->
							</div>
						</a>
						
					</div>
				{{/each}}
			</div>

			<!-- Footer -->
			<div class='footer'>

				<!-- Copyright -->
				<div id="copyright">
					<p>Built by: <span>Jared Suasin &copy 2022</span></p>
				</div>

