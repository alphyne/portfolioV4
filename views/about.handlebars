		<script>
			var canvasProject = document.getElementById('project-item');
			var ctxProject = canvasProject.getContext('2d');
			// Create an image element
			//var img = document.createElement('IMG');
			// When the image is loaded, draw it
			//img.onload = function () {
			// Create a shape
			var notchSize = 20;
			ctxProject.strokeStyle='rgb(255,255,255)';
			ctxProject.lineWidth = 2;
			ctxProject.beginPath();
			ctxProject.moveTo(0, 0);
			ctxProject.lineTo(0, ctxProject.canvasProject.height);
			ctxProject.lineTo(ctxProject.canvasProject.width - notchSize, ctxProject.canvasProject.height);
			ctxProject.lineTo(ctxProject.canvasProject.width, ctxProject.canvasProject.height - notchSize);
			ctxProject.lineTo(ctxProject.canvasProject.width, 0);
			ctxProject.lineTo(0, 0);
			ctxProject.closePath();
			// Clip to the current path
			ctxProject.clip();
			//ctx.drawImage(img, 0, 0);
			ctxProject.stroke();		
		</script>

